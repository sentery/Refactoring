<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="gast" nsURI="http://www.fzi.de/gast" nsPrefix="gast">
  <eSubpackages name="statements" nsURI="http://www.fzi.de/gast/statements" nsPrefix="statements">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="This package contains all classes and interfaces which belong to the statement hierarchy or are otherwise related to it. Statements describe the control flow within a function body."/>
    </eAnnotations>
    <eClassifiers xsi:type="ecore:EClass" name="ExceptionHandler" eSuperTypes="#//statements/Statement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This class represents handlings of exceptions. A typical exception handling consists of a guarded block, also known as try block, and several catch blocks. An optional finally block can be contained."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="catchBlocks" ordered="false"
          upperBound="-1" eType="#//statements/CatchBlock" containment="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents a list of the catch blocks of this exception handler."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="finallyBlock" ordered="false"
          eType="#//statements/BlockStatement" containment="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents a reference to the finally block of this exception handler. If there is no finally block then this property will be &quot;null&quot;."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="guardedBlock" ordered="false"
          lowerBound="1" eType="#//statements/BlockStatement" containment="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents a reference to the block statement which is guarded by this exception handler."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Statement" abstract="true" eSuperTypes="#//core/SourceEntity">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The Statement class is the root of the statement hierarchy and represents a statement, which can occur inside a function body. Non-local class declarations, imports and other object-oriented language statements, which cannot occur inside a function body, are not modelled."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="accesses" ordered="false"
          upperBound="-1" eType="#//accesses/BaseAccess" containment="true" eOpposite="#//accesses/BaseAccess/parentStatement">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents a list of accesses within this statement."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="cloneInstance" ordered="false"
          eType="#//annotations/CloneInstance" eOpposite="#//annotations/CloneInstance/statements"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="blockstatement" ordered="false"
          eType="#//statements/BlockStatement" eOpposite="#//statements/BlockStatement/statements"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="surroundingStatement"
          ordered="false" eType="#//statements/Statement" changeable="false" volatile="true"
          transient="true" derived="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents a reference to the surrounding statement of this statement. If there is no surrounding statement this property is &quot;null&quot;."/>
        </eAnnotations>
        <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
          <details key="name" value=""/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="numberOfStatements" ordered="false"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents the number of statements within this statement."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="maximumNestingLevel"
          ordered="false" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents the maximum nesting level of the control-flow structure within the statement, with respect to the depth of encapsulation of statements. "/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="numberOfComments" ordered="false"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents the number of comments attached to this statement."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="linesOfCode" ordered="false"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents the number of code lines that belong to this statement."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="numberOfEdgesInCFG" ordered="false"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents the number of edges in the control-flow graph of the statement."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="numberOfNodesInCFG" ordered="false"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents the number of nodes in the control-flow graph of the statement."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="branch" ordered="false"
          eType="#//statements/Branch" eOpposite="#//statements/Branch/statement"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="loopstatement" ordered="false"
          eType="#//statements/LoopStatement" eOpposite="#//statements/LoopStatement/body"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="BlockStatement" eSuperTypes="#//statements/Statement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The BlockStatement class models a composition statement or a function body. It also represents synchronized blocks."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="synchronized" ordered="false"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property is true if this block statement represents a synchronized block."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="statements" ordered="false"
          upperBound="-1" eType="#//statements/Statement" containment="true" eOpposite="#//statements/Statement/blockstatement">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property is an ordered list of statements inside this block statement."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="surroundingFunction"
          ordered="false" eType="#//functions/Function" eOpposite="#//functions/Function/body">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="If this block statement is a function body this is property is a reference to the function. Otherwise it is &quot;null&quot;."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Branch" eSuperTypes="#//core/SourceEntity">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This class represents a branch of a BranchStatement."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="conditionExpression"
          ordered="false" eType="#//statements/GASTExpression" containment="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="&lt;p>&#xD;&#xA;    This&amp;nbsp;property&amp;nbsp;represents&amp;nbsp;the&amp;nbsp;condition&amp;nbsp;expression&amp;nbsp;for&amp;nbsp;this&amp;nbsp;branch. Set to&#xD;&#xA;    optional only due to tool support: Expressions are currently not always extracted.&#xD;&#xA;&lt;/p>"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="branchstatement" ordered="false"
          lowerBound="1" eType="#//statements/BranchStatement" eOpposite="#//statements/BranchStatement/branches"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="statement" ordered="false"
          lowerBound="1" eType="#//statements/Statement" containment="true" eOpposite="#//statements/Statement/branch">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents the statement which represents the body of this branch."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="GASTExpression" abstract="true" eSuperTypes="#//core/SourceEntity">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This class is the root class of the expression hierarchy."/>
      </eAnnotations>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="BranchStatement" eSuperTypes="#//statements/Statement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This class models a branch, also known as selection statements or conditional statements. Typical examples are if statements or switch statements. "/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="branches" ordered="false"
          lowerBound="1" upperBound="-1" eType="#//statements/Branch" containment="true"
          eOpposite="#//statements/Branch/branchstatement">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents a list of branches."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="LoopStatement" eSuperTypes="#//statements/Statement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The LoopStatement class models all kinds of looping statements, which can occur in considered object-oriented programming languages, e. g. for, while, do-while or foreach. "/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="kind" ordered="false"
          lowerBound="1" eType="#//statements/LoopStatementKind"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="breakConditionExpression"
          ordered="false" eType="#//statements/GASTExpression" containment="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents a reference to the expression which represents the break condition of this loop statement."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="initExpression" ordered="false"
          eType="#//statements/GASTExpression" containment="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents the initialization expression of the loop. This property is optional."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="incrementExpression"
          ordered="false" eType="#//statements/GASTExpression" containment="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents the incrementation expression of the loop. This property is optional."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="body" ordered="false"
          lowerBound="1" eType="#//statements/Statement" containment="true" eOpposite="#//statements/Statement/loopstatement">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents a reference to the statement which represents the body of this loop statement."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="LoopStatementKind">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This enumeration represents the kinds of loop statements."/>
      </eAnnotations>
      <eLiterals name="FOREACH">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This literal shows that a loop statement represents a for-each loop."/>
        </eAnnotations>
      </eLiterals>
      <eLiterals name="WHILE" value="1">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This literal shows that a loop statement represents a while loop."/>
        </eAnnotations>
      </eLiterals>
      <eLiterals name="DOWHILE" value="2">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This literal shows that a loop statement represents a do-while loop."/>
        </eAnnotations>
      </eLiterals>
      <eLiterals name="FOR" value="3">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This literal shows that a loop statement represents a for loop."/>
        </eAnnotations>
      </eLiterals>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="CatchBlock" eSuperTypes="#//statements/BlockStatement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This class represents a catch block. It extends the BlockStatement class and has a catch parameter."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="catchParameter" ordered="false"
          lowerBound="1" eType="#//variables/CatchParameter" containment="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents a reference to the catch parameter for this catch block."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="JumpStatement" eSuperTypes="#//statements/Statement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This class models all kinds of non-conditional jumps. Throwing of exceptions and return-statements are also categorized as non-conditional jumps. "/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="kind" ordered="false"
          lowerBound="1" eType="#//statements/JumpStatementKind">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property is a reference to the kind of the jump statement."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="expression" ordered="false"
          eType="#//statements/GASTExpression" containment="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents a expression contained in this JumpStatement, e. g. body of a return statement, such as &quot;a+b&quot; in &quot;return a+b;&quot;."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="JumpStatementKind">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This enumeration represents the kinds of jump statements."/>
      </eAnnotations>
      <eLiterals name="JUMP" value="74">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This literal shows that a statement represents a local jump within the same function body. Typical examples are break, continue, goto, etc."/>
        </eAnnotations>
      </eLiterals>
      <eLiterals name="RETURN" value="73">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This literal shows that a statement represents a return statement of a function."/>
        </eAnnotations>
      </eLiterals>
      <eLiterals name="THROW" value="72">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This literal shows that this statement represents the throwing of an exception."/>
        </eAnnotations>
      </eLiterals>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SimpleStatement" eSuperTypes="#//statements/Statement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The SimpleStatement class represents a statement, whose control-flow is a linear sequence, for example variable assignments.&#xD;&#xA;"/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="expression" ordered="false"
          eType="#//statements/GASTExpression" containment="true">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents an expression contained in the statement."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="GASTBehaviour">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This class represents the behaviour of a service described by G-AST meta-model. It enables description of service behaviour in terms of control-flow and data-flow via model elements of the G-AST meta-model."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="blockstatement" ordered="false"
          lowerBound="1" eType="#//statements/BlockStatement">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="This property represents the blockstatement. The control-flow of the behaviour is modelled via the statement structure."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="core" nsURI="http://www.fzi.de/gast/core" nsPrefix="core">
    <eClassifiers xsi:type="ecore:EClass" name="BasePath" eSuperTypes="#//core/ModelElement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The plattform spefic root path containing the software internal directories. Instances of this element encapsulate the&#xD;&#xA;absolute path on the file system to the top level directory of the implementation of the software represented in the GAST&#xD;&#xA;model."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="root" ordered="false"
          lowerBound="1" eType="#//core/Root" eOpposite="#//core/Root/basePaths"/>
